<a name="l01341"></a>01341 <span class="comment">//* \brief  Set the RTT prescaler according to the TimeBase in ms</span>
<a name="l01342"></a>01342 <span class="comment">//*--------------------------------------------------------------------------------------</span>
<a name="l01343"></a><a class="code" href="lib__AT91SAM7X256_8h.html#38802b73a0666455ceb4098e85e360a9">01343</a> __inline <span class="keywordtype">unsigned</span> <span class="keywordtype">int</span> <a class="code" href="lib__AT91SAM7S256_8h.html#38802b73a0666455ceb4098e85e360a9">AT91F_RTTSetTimeBase</a>(
<a name="l01344"></a>01344         <a class="code" href="struct__AT91S__RTTC.html">AT91PS_RTTC</a> pRTTC, 
<a name="l01345"></a>01345         <span class="keywordtype">unsigned</span> <span class="keywordtype">int</span> ms)
<a name="l01346"></a>01346 {
<a name="l01347"></a>01347         <span class="keywordflow">if</span> (ms &gt; 2000)
<a name="l01348"></a>01348                 <span class="keywordflow">return</span> 1;   <span class="comment">// AT91C_TIME_OUT_OF_RANGE</span>
<a name="l01349"></a>01349         pRTTC-&gt;<a class="code" href="struct__AT91S__RTTC.html#f7214be994c52cf0ae55e15e2ab9c9e2">RTTC_RTMR</a> &amp;= ~0xFFFF;    
<a name="l01350"></a>01350         pRTTC-&gt;<a class="code" href="struct__AT91S__RTTC.html#f7214be994c52cf0ae55e15e2ab9c9e2">RTTC_RTMR</a> |= (((ms &lt;&lt; 15) /1000) &amp; 0xFFFF);      
<a name="l01351"></a>01351         <span class="keywordflow">return</span> 0;
<a name="l01352"></a>01352 }
<a name="l01353"></a>01353 
