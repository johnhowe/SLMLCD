<a name="l01952"></a>01952 <span class="preprocessor">                       AT91C_US_PAR_NONE + \</span>
<a name="l01953"></a>01953 <span class="preprocessor">                       AT91C_US_CHRL_8_BITS + \</span>
<a name="l01954"></a>01954 <span class="preprocessor">                       AT91C_US_CLKS_CLOCK )</span>
<a name="l01955"></a>01955 <span class="preprocessor"></span>
<a name="l01956"></a>01956 <span class="comment">//* SCK used Label</span>
<a name="l01957"></a><a class="code" href="lib__AT91SAM7X256_8h.html#519c9397a1fd5b6bc391f8aa72a35d56">01957</a> <span class="preprocessor">#define AT91C_US_SCK_USED (AT91C_US_CKLO | AT91C_US_CLKS_EXT)</span>
<a name="l01958"></a>01958 <span class="preprocessor"></span>
<a name="l01959"></a>01959 <span class="comment">//* Standard ISO T=0 Mode : 8 bits , 1 stop , parity</span>
<a name="l01960"></a><a class="code" href="lib__AT91SAM7X256_8h.html#7c0bdd5100cfd7c76188b918155a8598">01960</a> <span class="preprocessor">#define AT91C_US_ISO_READER_MODE ( AT91C_US_USMODE_ISO7816_0 + \</span>
<a name="l01961"></a>01961 <span class="preprocessor">                                                         AT91C_US_CLKS_CLOCK +\</span>
<a name="l01962"></a>01962 <span class="preprocessor">                                 AT91C_US_NBSTOP_1_BIT + \</span>
<a name="l01963"></a>01963 <span class="preprocessor">                                 AT91C_US_PAR_EVEN + \</span>
<a name="l01964"></a>01964 <span class="preprocessor">                                 AT91C_US_CHRL_8_BITS + \</span>
<a name="l01965"></a>01965 <span class="preprocessor">                                 AT91C_US_CKLO +\</span>
<a name="l01966"></a>01966 <span class="preprocessor">                                 AT91C_US_OVER)</span>
<a name="l01967"></a>01967 <span class="preprocessor"></span>
<a name="l01968"></a>01968 <span class="comment">//* Standard IRDA mode</span>
<a name="l01969"></a><a class="code" href="lib__AT91SAM7X256_8h.html#40f7d0c7c74ed585b4f6db2ab368b64e">01969</a> <span class="preprocessor">#define AT91C_US_ASYNC_IRDA_MODE (  AT91C_US_USMODE_IRDA + \</span>
<a name="l01970"></a>01970 <span class="preprocessor">                            AT91C_US_NBSTOP_1_BIT + \</span>
<a name="l01971"></a>01971 <span class="preprocessor">                            AT91C_US_PAR_NONE + \</span>
<a name="l01972"></a>01972 <span class="preprocessor">                            AT91C_US_CHRL_8_BITS + \</span>
<a name="l01973"></a>01973 <span class="preprocessor">                            AT91C_US_CLKS_CLOCK )</span>
<a name="l01974"></a>01974 <span class="preprocessor"></span>
<a name="l01975"></a>01975 <span class="comment">//*----------------------------------------------------------------------------</span>
<a name="l01976"></a>01976 <span class="comment">//* \fn    AT91F_US_Baudrate</span>
<a name="l01977"></a>01977 <span class="comment">//* \brief Caluculate baud_value according to the main clock and the baud rate</span>
<a name="l01978"></a>01978 <span class="comment">//*----------------------------------------------------------------------------</span>
<a name="l01979"></a><a class="code" href="lib__AT91SAM7X256_8h.html#afb45f22b05851b84133c43e975989ad">01979</a> __inline <span class="keywordtype">unsigned</span> <span class="keywordtype">int</span> <a class="code" href="lib__AT91SAM7S256_8h.html#afb45f22b05851b84133c43e975989ad">AT91F_US_Baudrate</a> (
<a name="l01980"></a>01980         <span class="keyword">const</span> <span class="keywordtype">unsigned</span> <span class="keywordtype">int</span> main_clock, <span class="comment">// \arg peripheral clock</span>
<a name="l01981"></a>01981         <span class="keyword">const</span> <span class="keywordtype">unsigned</span> <span class="keywordtype">int</span> baud_rate)  <span class="comment">// \arg UART baudrate</span>
<a name="l01982"></a>01982 {
<a name="l01983"></a>01983         <span class="keywordtype">unsigned</span> <span class="keywordtype">int</span> baud_value = ((main_clock*10)/(baud_rate * 16));
<a name="l01984"></a>01984         <span class="keywordflow">if</span> ((baud_value % 10) &gt;= 5)
<a name="l01985"></a>01985                 baud_value = (baud_value / 10) + 1;
<a name="l01986"></a>01986         <span class="keywordflow">else</span>
<a name="l01987"></a>01987                 baud_value /= 10;
<a name="l01988"></a>01988         <span class="keywordflow">return</span> baud_value;
<a name="l01989"></a>01989 }
