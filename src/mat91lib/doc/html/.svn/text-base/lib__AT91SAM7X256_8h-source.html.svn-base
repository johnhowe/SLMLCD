<a name="l02301"></a>02301 <span class="comment">//* \fn    AT91F_SSC_SetBaudrate</span>
<a name="l02302"></a>02302 <span class="comment">//* \brief Set the baudrate according to the CPU clock</span>
<a name="l02303"></a>02303 <span class="comment">//*----------------------------------------------------------------------------</span>
<a name="l02304"></a><a class="code" href="lib__AT91SAM7X256_8h.html#e9e3b487a5fa77d67c7602dad996e0a7">02304</a> __inline <span class="keywordtype">void</span> <a class="code" href="lib__AT91SAM7S256_8h.html#e9e3b487a5fa77d67c7602dad996e0a7">AT91F_SSC_SetBaudrate</a> (
<a name="l02305"></a>02305         <a class="code" href="struct__AT91S__SSC.html">AT91PS_SSC</a> pSSC,        <span class="comment">// \arg pointer to a SSC controller</span>
<a name="l02306"></a>02306         <span class="keywordtype">unsigned</span> <span class="keywordtype">int</span> mainClock, <span class="comment">// \arg peripheral clock</span>
<a name="l02307"></a>02307         <span class="keywordtype">unsigned</span> <span class="keywordtype">int</span> speed)     <span class="comment">// \arg SSC baudrate</span>
<a name="l02308"></a>02308 {
<a name="l02309"></a>02309         <span class="keywordtype">unsigned</span> <span class="keywordtype">int</span> baud_value;
<a name="l02310"></a>02310         <span class="comment">//* Define the baud rate divisor register</span>
<a name="l02311"></a>02311         <span class="keywordflow">if</span> (speed == 0)
<a name="l02312"></a>02312            baud_value = 0;
<a name="l02313"></a>02313         <span class="keywordflow">else</span>
<a name="l02314"></a>02314         {
<a name="l02315"></a>02315            baud_value = (<span class="keywordtype">unsigned</span> int) (mainClock * 10)/(2*speed);
<a name="l02316"></a>02316            <span class="keywordflow">if</span> ((baud_value % 10) &gt;= 5)
<a name="l02317"></a>02317                   baud_value = (baud_value / 10) + 1;
<a name="l02318"></a>02318            <span class="keywordflow">else</span>
<a name="l02319"></a>02319                   baud_value /= 10;
<a name="l02320"></a>02320         }
<a name="l02321"></a>02321 
<a name="l02322"></a>02322         pSSC-&gt;<a class="code" href="struct__AT91S__SSC.html#084f2cf43aa620b87b685b910eee8929">SSC_CMR</a> = baud_value;
<a name="l02323"></a>02323 }
