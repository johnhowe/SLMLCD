<a name="l01858"></a>01858 <span class="comment">//* \fn    AT91F_SPI_Close</span>
<a name="l01859"></a>01859 <span class="comment">//* \brief Close SPI: disable IT disable transfert, close PDC</span>
<a name="l01860"></a>01860 <span class="comment">//*----------------------------------------------------------------------------</span>
<a name="l01861"></a><a class="code" href="lib__AT91SAM7X256_8h.html#1d538178f15852104ef39cbb837504c3">01861</a> __inline <span class="keywordtype">void</span> <a class="code" href="lib__AT91SAM7S256_8h.html#1d538178f15852104ef39cbb837504c3">AT91F_SPI_Close</a> (
<a name="l01862"></a>01862         <a class="code" href="struct__AT91S__SPI.html">AT91PS_SPI</a> pSPI)     <span class="comment">// \arg pointer to a SPI controller</span>
<a name="l01863"></a>01863 {
<a name="l01864"></a>01864     <span class="comment">//* Reset all the Chip Select register</span>
<a name="l01865"></a>01865     pSPI-&gt;<a class="code" href="struct__AT91S__SPI.html#bbf4500576969ea8013cca841d0b1979">SPI_CSR</a>[0] = 0 ;
<a name="l01866"></a>01866     pSPI-&gt;<a class="code" href="struct__AT91S__SPI.html#bbf4500576969ea8013cca841d0b1979">SPI_CSR</a>[1] = 0 ;
<a name="l01867"></a>01867     pSPI-&gt;<a class="code" href="struct__AT91S__SPI.html#bbf4500576969ea8013cca841d0b1979">SPI_CSR</a>[2] = 0 ;
<a name="l01868"></a>01868     pSPI-&gt;<a class="code" href="struct__AT91S__SPI.html#bbf4500576969ea8013cca841d0b1979">SPI_CSR</a>[3] = 0 ;
<a name="l01869"></a>01869 
<a name="l01870"></a>01870     <span class="comment">//* Reset the SPI mode</span>
<a name="l01871"></a>01871     pSPI-&gt;<a class="code" href="struct__AT91S__SPI.html#53c8dfe2e423e8032df8655ea1c55ae3">SPI_MR</a> = 0  ;
<a name="l01872"></a>01872 
<a name="l01873"></a>01873     <span class="comment">//* Disable all interrupts</span>
<a name="l01874"></a>01874     pSPI-&gt;<a class="code" href="struct__AT91S__SPI.html#8b1413ef206fd57fcd3dbedaeebbb0a5">SPI_IDR</a> = 0xFFFFFFFF ;
<a name="l01875"></a>01875 
<a name="l01876"></a>01876     <span class="comment">//* Abort the Peripheral Data Transfers</span>
<a name="l01877"></a>01877     <a class="code" href="lib__AT91SAM7S256_8h.html#e7a00663d9b586d0060d83e4c3e1921f">AT91F_PDC_Close</a>((<a class="code" href="struct__AT91S__PDC.html">AT91PS_PDC</a>) &amp;(pSPI-&gt;<a class="code" href="struct__AT91S__SPI.html#4a254f51f4a9ba0d9950b98e21477a96">SPI_RPR</a>));
<a name="l01878"></a>01878 
<a name="l01879"></a>01879     <span class="comment">//* Disable receiver and transmitter and stop any activity immediately</span>
<a name="l01880"></a>01880     pSPI-&gt;<a class="code" href="struct__AT91S__SPI.html#5d6553c05d0a9468b53a3521e9dc2d03">SPI_CR</a> = <a class="code" href="AT91SAM7S256_8h.html#cc6bb7762dad073f7f4b19abf04a389a">AT91C_SPI_SPIDIS</a>;
<a name="l01881"></a>01881 }
