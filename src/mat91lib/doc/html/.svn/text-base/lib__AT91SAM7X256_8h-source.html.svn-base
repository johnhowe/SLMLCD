<a name="l03376"></a>03376 <span class="comment">//* \fn    AT91F_ADC_CfgTimings</span>
<a name="l03377"></a>03377 <span class="comment">//* \brief Configure the different necessary timings of the ADC controller</span>
<a name="l03378"></a>03378 <span class="comment">//*----------------------------------------------------------------------------</span>
<a name="l03379"></a><a class="code" href="lib__AT91SAM7X256_8h.html#939952923f1e71ff535241e42e8c35d4">03379</a> __inline <span class="keywordtype">void</span> <a class="code" href="lib__AT91SAM7S256_8h.html#939952923f1e71ff535241e42e8c35d4">AT91F_ADC_CfgTimings</a> (
<a name="l03380"></a>03380         <a class="code" href="struct__AT91S__ADC.html">AT91PS_ADC</a> pADC, <span class="comment">// pointer to a ADC controller</span>
<a name="l03381"></a>03381         <span class="keywordtype">unsigned</span> <span class="keywordtype">int</span> mck_clock, <span class="comment">// in MHz </span>
<a name="l03382"></a>03382         <span class="keywordtype">unsigned</span> <span class="keywordtype">int</span> adc_clock, <span class="comment">// in MHz </span>
<a name="l03383"></a>03383         <span class="keywordtype">unsigned</span> <span class="keywordtype">int</span> startup_time, <span class="comment">// in us </span>
<a name="l03384"></a>03384         <span class="keywordtype">unsigned</span> <span class="keywordtype">int</span> sample_and_hold_time)      <span class="comment">// in ns  </span>
<a name="l03385"></a>03385 {
<a name="l03386"></a>03386         <span class="keywordtype">unsigned</span> <span class="keywordtype">int</span> prescal,startup,shtim;
<a name="l03387"></a>03387         
<a name="l03388"></a>03388         prescal = mck_clock/(2*adc_clock) - 1;
<a name="l03389"></a>03389         startup = adc_clock*startup_time/8 - 1;
<a name="l03390"></a>03390         shtim = adc_clock*sample_and_hold_time/1000 - 1;
<a name="l03391"></a>03391         
<a name="l03392"></a>03392         <span class="comment">//* Write to the MR register</span>
<a name="l03393"></a>03393         pADC-&gt;<a class="code" href="struct__AT91S__ADC.html#8803317070c9bb96c65a5cc06e3580c8">ADC_MR</a> = ( (prescal&lt;&lt;8) &amp; <a class="code" href="AT91SAM7S256_8h.html#666903aab4d6d173d6a2be5cbee5a91a">AT91C_ADC_PRESCAL</a>) | ( (startup&lt;&lt;16) &amp; <a class="code" href="AT91SAM7S256_8h.html#d7ac5ddb52d3901392845a7785966639">AT91C_ADC_STARTUP</a>) | ( (shtim&lt;&lt;24) &amp; <a class="code" href="AT91SAM7S256_8h.html#9aa84fa92b822b39846338d8cccf74f7">AT91C_ADC_SHTIM</a>);
<a name="l03394"></a>03394 }
