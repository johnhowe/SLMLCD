<a name="l01176"></a>01176         <span class="keywordtype">unsigned</span> <span class="keywordtype">int</span> reg = pPMC-&gt;<a class="code" href="struct__AT91S__PMC.html#7172b8a93b54c122f4e7ca6c6aa6520c">PMC_MCKR</a>;
<a name="l01177"></a>01177         <span class="keywordtype">unsigned</span> <span class="keywordtype">int</span> prescaler = (1 &lt;&lt; ((reg &amp; <a class="code" href="AT91SAM7S256_8h.html#5008f2f510effd06886208cf385e03d5">AT91C_PMC_PRES</a>) &gt;&gt; 2));
<a name="l01178"></a>01178         <span class="keywordtype">unsigned</span> <span class="keywordtype">int</span> pllDivider, pllMultiplier;
<a name="l01179"></a>01179 
<a name="l01180"></a>01180         <span class="keywordflow">switch</span> (reg &amp; <a class="code" href="AT91SAM7S256_8h.html#aa04778b864187f4ef69c232d5624c08">AT91C_PMC_CSS</a>) {
<a name="l01181"></a>01181                 <span class="keywordflow">case</span> <a class="code" href="AT91SAM7S256_8h.html#b296199ccb1353e23d03f34b8278e2e0">AT91C_PMC_CSS_SLOW_CLK</a>: <span class="comment">// Slow clock selected</span>
